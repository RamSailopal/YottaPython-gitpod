{
    "files.associations": {
        "*.m": "mumps"
    },
    "code-runner.executorMap": {
        "mumps": "docker exec -i yottadb /bin/bash -c '/opt/yottadb/current/ydb <<< \"ZL \\\"$fileName\\\" D ^$fileNameWithoutExt\"'",
        "python": "bash -c \"filedirName1=${filedirName} filedirName1=$(echo $filedirName1 | sed 's#^/workspace/YottaPython-gitpod##') && echo $filedirName1/${fileName}\""
    }
}



