{
    "files.associations": {
        "*.m": "mumps"
    },
    "code-runner.executorMap": {
        "mumps": "docker exec -i yottadb /bin/bash -c '/opt/yottadb/current/ydb <<< \"ZL \\\"$fileName\\\" D ^$fileNameWithoutExt\"'",
        "python": "docker exec -i yottadb python3 ${filedirName/\\/workspace\\/YottaPython-gitpod/}/${fileName}"
    }
}



