{
    "files.associations": {
        "*.m": "mumps"
    },
    "code-runner.executorMap": {
        "mumps": "docker exec -i yottadb /bin/bash -c '/opt/yottadb/current/ydb <<< \"ZL \\\"$fileName\\\" D ^$fileNameWithoutExt\"'",
        "python": "fullpath=${filedirName}/${fileName} && fullpath1=`echo $fullpath | sed 's#work#gerk#'` $$ echo fullpath1"
    }
}



